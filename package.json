{
  "name": "prepaid-merchant-portal",
  "private": true,
  "version": "1.0.0",
  "description": "Prepaid+ Merchant Portal",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint src",
    "check": "npm run format:check && npm run lint",
    "start": "npm run dev",
    "dev": "npm run check && cross-env NODE_ENV=development vite",
    "build": "npm run check && cross-env NODE_ENV=production vite build",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
     "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test 'npm run dev' http://localhost:8080 'cypress run'",
    "test:all": "npm run test && npm run test:e2e"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "chart.js": "^4.4.9",
    "dom7": "^4.0.6",
    "framework7": "^8.3.4",
    "framework7-icons": "^5.0.5",
    "html-to-image": "^1.11.13",
    "http-server": "^14.1.1",
    "material-icons": "^1.13.12",
    "skeleton-elements": "^4.0.1",
    "swiper": "^11.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@rollup/plugin-replace": "^6.0.2",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.5.3",
    "rollup-plugin-framework7": "^1.2.1",
    "vite": "^6.0.7",
    "vite-plugin-static-copy": "^2.2.0",
    "jest": "^29.0.0",
    "babel-jest": "^29.0.0",
    "@babel/preset-env": "^7.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "cypress": "^12.0.0",
    "start-server-and-test": "^2.0.0",
    "fake-indexeddb": "^4.0.0"
  }
}
